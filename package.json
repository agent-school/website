{
  "name": "agent-school-website",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --webpack",
    "dev:webpack": "next dev --webpack",
    "dev:turbo": "next dev --turbopack",
    "dev:turbo-no-cache": "cross-env NEXT_TURBOPACK_PERSISTENT_CACHING=0 next dev",
    "dev:clean": "npm run clean:cache && npm run dev",
    "build": "next build --webpack",
    "build:turbo": "next build --turbopack",
    "start": "next start",
    "lint": "next lint",
    "clean:cache": "node -e \"try{require('fs').rmSync('.next',{recursive:true,force:true})}catch(e){}\""
  },
  "dependencies": {
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-slider": "^1.3.6",
    "@radix-ui/react-slot": "^1.2.4",
    "@radix-ui/react-tabs": "^1.1.13",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "framer-motion": "^12.0.0",
    "lottie-react": "^2.4.0",
    "lucide-react": "^0.468.0",
    "next": "^16.1.0",
    "next-themes": "^0.4.6",
    "react": "^19",
    "react-dom": "^19",
    "tailwind-merge": "^3.4.0",
    "tailwindcss-animate": "^1.0.7"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@types/node": "^22",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "cross-env": "^10.1.0",
    "eslint": "^9",
    "eslint-config-next": "16.1.0",
    "postcss": "^8",
    "tailwindcss": "^3.4.1",
    "typescript": "^5"
  }
}
